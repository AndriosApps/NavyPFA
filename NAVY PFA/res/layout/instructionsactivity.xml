<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="match_parent" 
  android:orientation="vertical" 
  android:id="@+id/ll"
  android:background="@drawable/background">
    <LinearLayout android:layout_height="wrap_content" android:layout_width="match_parent"  android:id="@+id/linearLayout1" android:gravity="center">
        <TextView android:layout_height="wrap_content" android:text="2011 Navy Physical Fitness Standards" android:id="@+id/tv" android:textSize="18dip" android:layout_width="wrap_content"></TextView>
    </LinearLayout>
    <ScrollView android:layout_width="match_parent" android:id="@+id/scrollView1" android:layout_height="wrap_content" android:layout_weight="1">
        <LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout9" android:layout_height="match_parent" android:orientation="vertical">
            <LinearLayout android:layout_weight="1" android:id="@+id/linearLayout2" android:layout_width="match_parent" android:gravity="center" android:orientation="horizontal" android:layout_height="50dip">
                <LinearLayout android:id="@+id/linearLayout11" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_weight="1" android:gravity="center">
                    <LinearLayout android:id="@+id/LinearLayout03" android:layout_width="40dip" android:layout_height="20dip" android:background="@drawable/new_icon"></LinearLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearLayout3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="center" android:orientation="vertical">
                    <TextView android:id="@+id/instructionActivityOPNAVLBL" android:textColor="#FFFF00" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="OPNAV 6110.1J"></TextView>
                    <TextView android:id="@+id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Official PRT/PFA Instruction"></TextView>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearLayout10" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_weight="1" android:gravity="center">
                    <LinearLayout android:id="@+id/linearLayout8" android:layout_width="40dip" android:layout_height="20dip" android:background="@drawable/new_icon"></LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:layout_weight="1" android:id="@+id/ll2" android:layout_width="match_parent" android:gravity="center" android:orientation="vertical" android:layout_height="50dip">
                <TextView android:id="@+id/instructionActivityNAVADMIN1LBL" android:textColor="#FFFF00" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="NAVADMIN 180-05"></TextView>
                <TextView android:id="@+id/textView2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Authorizes ADSEP for PRT failures"></TextView>
            </LinearLayout>
            <LinearLayout android:layout_weight="1" android:id="@+id/linearLayout4" android:layout_width="match_parent" android:gravity="center" android:orientation="vertical" android:layout_height="50dip">
                <TextView android:id="@+id/instructionActivityNAVADMIN2LBL" android:textColor="#FFFF00" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="NAVADMIN 293-06"></TextView>
                <TextView android:id="@+id/textView3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Authorizes Elliptical for PRT use"></TextView>
            </LinearLayout>
            <LinearLayout android:layout_weight="1" android:id="@+id/LinearLayout01" android:layout_width="match_parent" android:gravity="center" android:orientation="vertical" android:layout_height="50dip">
                <TextView android:id="@+id/instructionActivityNAVADMIN3LBL" android:textColor="#FFFF00" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="NAVADMIN 011/07"></TextView>
                <TextView android:id="@+id/TextView02" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Stationary Cycle and Elliptical Procedures"></TextView>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
    <LinearLayout android:layout_height="wrap_content" android:layout_width="match_parent" android:id="@+id/linearLayout5" android:layout_weight="1" android:gravity="center" android:orientation="vertical">
        <TextView android:text="Comments and Ratings help us improve this app.  Please take a minute to rate it." android:id="@+id/textView4" android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="center"></TextView>
    </LinearLayout>
    <LinearLayout android:gravity="center" android:layout_weight="1" android:layout_width="match_parent" android:id="@+id/LinearLayout02" android:layout_height="wrap_content" android:orientation="horizontal">
        <LinearLayout android:layout_width="wrap_content" android:id="@+id/linearLayout6" android:layout_height="match_parent" android:layout_weight="1" android:gravity="center">
            <Button android:text="Rate Now" android:textColor="#ffffff" android:layout_width="wrap_content" android:background="@drawable/grey_button" android:layout_height="50dip" android:id="@+id/instructionActivityRateBTN"></Button>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout android:layout_width="match_parent" android:orientation="vertical" android:layout_weight="0" android:id="@+id/linearLayout5" android:layout_height="wrap_content" android:gravity="center|bottom">
    	<com.google.ads.AdView 
		xmlns:ads="http://schemas.android.com/apk/lib/com.google.ads"
		android:id="@+id/instructionsAdView"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        ads:adUnitId="a14debe274d41b2"
        ads:adSize="BANNER"
        android:layout_weight="0" />
    </LinearLayout>
</LinearLayout>
