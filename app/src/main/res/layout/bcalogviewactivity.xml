<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/background"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="fill_parent"
        android:layout_height="50dip"
        android:background="#000000"
        android:gravity="center">

        <TextView
            android:id="@+id/bcaLogViewActivityDateLBL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0"
            android:text="10 SEP 2011"
            android:textColor="#ffffff"
            android:textSize="22dip"></TextView>

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:orientation="horizontal"></LinearLayout>

        <Button
            android:id="@+id/bcaLogViewActivitySaveBTN"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0"
            android:background="@drawable/button_save"
            android:text=""></Button>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout14"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linearLayout5"
                android:layout_width="fill_parent"
                android:layout_height="150dip"
                android:gravity="center">

                <LinearLayout
                    android:id="@+id/linearLayout12"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:gravity="center|left"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/linearLayout20"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center">

                        <TextView
                            android:id="@+id/journalEntryViewActivityTitleTXT"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:gravity="center"
                            android:text="BCA"
                            android:textSize="22dip"></TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLayout21"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="horizontal"></LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearLayout13"
                    android:layout_width="150dip"
                    android:layout_height="fill_parent"
                    android:layout_weight="0"
                    android:gravity="center|left"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/linearLayout22"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center|left">

                        <CheckBox
                            android:id="@+id/bcaLogViewActivityImportantCheckBox"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:text="Official"></CheckBox>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLayout23"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center|left"
                        android:orientation="horizontal"></LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout4"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/linearLayout6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/LinearLayout19"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:id="@+id/LinearLayout17"
                            android:layout_width="75dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/TextView01"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Height"
                                android:textSize="18dip"></TextView>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/LinearLayout16"
                            android:layout_width="75dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/bcaLogViewActivityHeightLBL"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="5'9&quot; / 69&quot;"
                                android:textSize="18dip"></TextView>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/LinearLayout11"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:id="@+id/LinearLayout10"
                            android:layout_width="75dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/TextView08"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Weight"
                                android:textSize="18dip"></TextView>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/LinearLayout09"
                            android:layout_width="75dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/bcaLogViewActivityWeightLBL"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="210lbs"
                                android:textSize="18dip"></TextView>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/bcaLogViewActivityHeightWeightLL"
                    android:layout_width="125dip"
                    android:layout_height="50dip"
                    android:background="@drawable/grey_button"
                    android:orientation="horizontal"></LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/bcaLogViewActivityBodyfatStatsLL"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/LinearLayout02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/LinearLayout13"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:id="@+id/LinearLayout14"
                            android:layout_width="75dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/TextView10"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Neck"
                                android:textSize="18dip"></TextView>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/LL"
                            android:layout_width="75dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/bcaLogViewActivityNeckLBL"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="16&quot;"
                                android:textSize="18dip"></TextView>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/LinearLayout01"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:id="@+id/LinearLayout07"
                            android:layout_width="75dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/TextView05"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Waist"
                                android:textSize="18dip"></TextView>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/LinearLayout08"
                            android:layout_width="75dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/bcaLogViewActivityWaistLBL"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="32&quot;"
                                android:textSize="18dip"></TextView>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/LinearLayout06"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:id="@+id/LinearLayout05"
                            android:layout_width="75dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/bcaLogViewActivityHipsLBLLBL"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Hips"
                                android:textSize="18dip"></TextView>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/LinearLayout04"
                            android:layout_width="75dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/bcaLogViewActivityHipsLBL"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="35&quot;"
                                android:textSize="18dip"></TextView>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/bcaLogViewActivityBodyfatLL"
                    android:layout_width="125dip"
                    android:layout_height="50dip"
                    android:background="@drawable/grey_button"
                    android:orientation="horizontal"></LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
